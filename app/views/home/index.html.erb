
<table class="table">
  <thead>
    <tr>
      <th>Servicios</th>
      <th>Imagen</th>
      <th colspan="2"></th>
    </tr>
  </thead>
  <tbody>
      <% @ServiciosPvs.each  do |serpvs| %>
        <tr>
       
        <%@existe =  UsuarioServicio.where("msisdn = ? AND servicio = ?",@msisdn,serpvs.id).count%>
         
        <% if @existe == 0 %>
          <td><%=serpvs.nombre  %></td>
           <td> <%=link_to(image_tag(serpvs.id.to_s, size: "100x100"),"/suscripcion/"+serpvs.id.to_s ) %></td>
        <% end %>
        </tr>
      <% end %>
    </tbody>
  </table>
