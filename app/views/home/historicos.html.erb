<table class="table">
  <thead>
    <tr>
      <th>Servicios</th>
      <th>Imagen</th>
      <th>Fecha Baja</th>
      <th colspan="2"></th>
    </tr>
  </thead>
  <tbody>
      <% @Historicos.each  do |compras| %>
        <tr>
       
        <%@servicio = ServicioPv.where("idpvs = " + compras.servicio.to_s )%>
        <% @servicio.each  do |servpvs| %>
          <td><%=servpvs.nombre%></td>
          <% end %>
           <td> <%=link_to(image_tag(compras.servicio.to_s, size: "100x100"),"/desuscripcion/"+compras.servicio.to_s ) %></td>
           <td><%=compras.created_at%></td>
        </tr>
      <% end %>
    </tbody>
  </table>
